;;; Inflecting Lexical Rule Instances

plural-suffix :=
%suffix (* s)
plural-lex-rule.

saxon-genitive-suffix :=
%suffix (* 's)
saxon-genitive-lex-rule.

pst-suffix :=
%suffix (* ed)
pst-lex-rule.

pres-3sg-suffix :=
%suffix (* s)
pres-3sg-lex-rule.

pres-part-suffix :=
%suffix (* ing)
pres-part-lex-rule.

past-part-suffix :=
%suffix (* ed)
past-part-lex-rule.
